{
    "endpoints": [
        "0.0.0.0:8101"
    ],
    "backlog": 512,
    "threads": 16,
    "buffer_size": 65536,
	"logger": {
        "level": "info",
        "frontends": [
            {
                "formatter": {
                    "type": "string",
                    "pattern": "%(timestamp)s %(request_id)s/%(lwp)s/%(pid)s %(severity)s: %(message)s, %(...L)s"
                },
                "sink": {
                    "type": "files",
                    "path": "/dev/stdout",
                    "autoflush": true,
                    "rotation": { "move": 0 }
                }
            }
        ]
    },
    "daemon": {
        "fork": false,
        "uid": 1000
    },
    "monitor-port": 22334,
    "request_header": "X-Request",
    "trace_header": "X-Trace",
    "application": {
	    "language_detector_stats": "/home/zbr/awork/warp/build/lang.out"
    }
}
